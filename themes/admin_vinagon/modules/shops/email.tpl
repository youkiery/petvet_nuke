<!-- BEGIN: main --><table summary="" class="tab1" style="margin: 1px 0px">	<tbody>		<tr>			<td><input type="text" value="{q}" maxlength="64" name="q" id="idq"				style="width: 265px"> <input type="button" value="{LANG.search}"				onClick="search_email()"></td>		</tr>	</tbody></table><table summary="" class="tab1" style="margin-bottom: 5px">	<thead>		<tr>			<td align="center" width="20"><input name="check_all" id="checkall"				type="checkbox" /></td>			<td>Email</td>			<td width="200">Time</td>			<td width="100"></td>		</tr>	</thead>	<form action="" method="post"><input type="hidden" value="1"		name="save" />	<tbody{ROW.bg}>		<tr>			<td align="center">#</td>			<td><input type="text" value="{DATA.email}" name="email" id="email"				style="width: 98%"></td>			<td><input type="text" name="addtime" id="addtime" style="width: 98%"				readonly="readonly"></td>			<td><input type="submit" value="{LANG.save}" /></td>		</tr>	</tbody>	</form>	<!-- BEGIN: loop -->	<tbody{ROW.bg}>		<tr>			<td align="center"><input type="checkbox" value="{ROW.id}"				class="idlist" /></td>			<td><b>{ROW.email}</b></td>			<td>{ROW.addtime}</td>			<td align="center"><span class="edit_icon"><a href="{ROW.edit}">{LANG.edit}</a></span>			<span class="delete_icon"><a href="{ROW.del}" class="adel">{LANG.del}</a></span>			</td>		</tr>	</tbody>	<!-- END: loop -->	<tfoot>		<tr align="left">			<td colspan="2"><span class="delete_icon"><a href="#" class="delall">{LANG.del_select}</a></span>&nbsp;			</td>			<td colspan="4" align="right"><!-- BEGIN: page -->{generate_page}<!-- END: page --></td>		</tr>	</tfoot></table><script type="text/javascript">
clickcheckall();
delete_one('adel','{LANG.del_confim}','{URLBACK}');
delete_all('idlist','delall','{LANG.del_confim}','{LANG.no_select_items}','{DELALL}','{URLBACK}');
</script><!-- END: main -->